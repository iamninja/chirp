
<div class="row">
  <div class="span4">
    <%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name),html: {class: "well"}) do |f| %>


    <fieldset>
        <legend>Sign Up</legend>
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :profile_name %>
      <%= f.input :email %>
      <%= f.input :password%>
      <%= f.input :password_confirmation %>
    </fieldset>
      

      <%= f.button :submit, "Sign up" %>
    </div>
    <div class="span8">
    <h3>Registation powered by Devise</h3>
    <%= devise_error_messages!%></div>
    <% end %>



</div>
